[Unit]
Description=Racha Habito
After=syslog.target
After=network.target

[Service]
Type=simple
User=chepecarlos
Group=chepecarlos
ExecStart=/home/chepecarlos/.local/bin/rachahabitos
SyslogIdentifier=RachaHabito

# Give the script some time to startup
TimeoutSec=300

[Install]
WantedBy=multi-user.target